{
    "data_path": "./data/",
    "labels":[
        "Normal",
        "PTC HE",
        "UN",
        "fvptc",
        "FTC",
        "med",
        "poor",
        "und"
    ],
    
    "num_estimate": 10,
    "batch_size": 128,
    "epochs": 10,
    "img_resize": 224,

    "dataset_params":{
        "grayscale_flag": true,
        "normalize_per_img": false,
        "arrange": null,
        "train_mean": [0.485, 0.456, 0.406],
        "train_std": [0.229, 0.224, 0.225] ,
        "test_mean": [0.485, 0.456, 0.406],
        "test_std": [0.229, 0.224, 0.225]
    },

    "tissue_dataset_params": {
        "use": false,
        "phase": "train",
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
    },

    "net_params": {
        "name": "resnet101",
        "transfer_learning": true,
        "pretrained": true
    },

    "loss_weight_flag": true,
    
    "optim_params": {
        "name": "adam",
        "lr_not_pretrained": 1e-3,
        "lr_pretrained": 1e-4,
        "momentum": 0.9,
        "weight_decay": 1e-4
    }
}
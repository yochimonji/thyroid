{
    "data_path": {
        "train": "data/train/",
        "test": "data/test/",
        "tissue": "data/tissue array/train/"
    },

    "labels":[
        "Normal",
        "PTC HE",
        "UN",
        "fvptc",
        "FTC",
        "med",
        "poor",
        "und"
    ],
    
    "num_estimate": 10,
    "batch_size": 32,
    "epochs": 10,
    "img_resize": 224,

    "dataset_params":{
        "grayscale_flag": false,
        "normalize_per_img": false,
        "arrange": null,
        "train_mean": [0.485, 0.456, 0.406],
        "train_std": [0.229, 0.224, 0.225] ,
        "test_mean": [0.485, 0.456, 0.406],
        "test_std": [0.229, 0.224, 0.225]
    },

    "tissue_dataset_params": {
        "use": true,
        "phase": "train",
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
    },

    "net_params": {
        "name": "resnet50",
        "multi_net": false,
        "transfer_learning": false,
        "pretrained": true
    },

    "loss_weight_flag": true,
    
    "optim_params": {
        "name": "adam",
        "lr_not_pretrained": 1e-4,
        "lr_pretrained": 1e-5,
        "momentum": 0.9,
        "weight_decay": 1e-4
    }
}